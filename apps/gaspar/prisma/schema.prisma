datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Sensor {
  id        String   @id @default(uuid())
  name      String
  type     String
  isActive  Boolean
  activeSince DateTime?
  room      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
